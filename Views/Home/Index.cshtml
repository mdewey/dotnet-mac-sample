@model IEnumerable<CoffeeShop>
@{ 
    ViewData["Title"] = "My Favorite Coffee Shops"; 
}

<div class="jumbotron">
    <h1>
        @ViewData["Title"]
    </h1>
    <p><a class="btn btn-primary btn-lg" href="#" role="button">Add one</a></p>
</div>



@foreach(var shop in Model){
        
<div class="row">
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="@shop.Image" alt="...">
            <div class="caption">
                <h3>@shop.Name</h3>
                <p>@shop.Address</p>
            </div>
        </div>
    </div>
</div>

}
